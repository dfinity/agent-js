{
  "name": "@dfinity/agent-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Use an Agent to interact with the Internet Computer from your JavaScript program.",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-plugin-jsdoc": "^32.0.0",
    "lerna": "^3.22.1"
  },
  "scripts": {
    "preinstall": "echo Please run 'npx lerna bootstrap' instead of npm install.; exit 1",
    "prepare": "npm run build",
    "build": "lerna run build --stream",
    "test": "lerna run test --concurrency=1 --stream",
    "test:coverage": "lerna run test:coverage --parallel",
    "publish:release": "lerna publish --force-publish=* --no-push --no-git-tag-version",
    "lint": "npm run lint:packages",
    "lint:e2e": "eslint --cache --cache-location node_modules/.cache/eslint 'e2e/*/**/*.ts*'",
    "line:fix": "npm run lint -- --fix",
    "lint:packages": "eslint --cache --cache-location node_modules/.cache/eslint 'packages/*/src/**/*.ts*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfinity-lab/agent-js.git"
  },
  "author": "DFINITY Stiftung <sdk@dfinity.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dfinity-lab/agent-js/issues"
  },
  "homepage": "https://github.com/dfinity-lab/agent-js#readme"
}
