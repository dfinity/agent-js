{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "target": "es6",
    "lib": [
      "dom",
      "es2017"
    ],
    "declaration": true,
    "sourceMap": true,
    "rootDir": "./",
    "composite": true,
    "tsBuildInfoFile": "./build_info.json",
    "strict": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "types/*",
    "src/**/*.ts"
  ]
}
